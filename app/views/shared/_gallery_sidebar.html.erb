<div class="row">
  <div class="col">
    <div class="card">
      <div class="card-body">
        <div class="mb-2">
          <b>Galleries</b>
        </div>
        <% galleries.each do |gallery| %>
          <div class="mb-1">
            <%= link_to gallery.title, photo_gallery_path(gallery.id) %>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>
